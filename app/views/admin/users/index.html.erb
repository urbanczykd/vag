<table>
  <tr>
    <th>#id</th>
    <th>username</th>
    <th>email</th>

    <th colspan="3">options</th>
  </tr>
<% @users.each do |user| %>
<tr>
  <td>#<%= user.id %></td>
  <td><%= user.username %></td>
  <td><%= user.email %></td>
    <td><%= link_to "Pokaż", admin_user_path(user)%></td>
    <td><%= link_to "Edytuj", edit_admin_user_path(user) %></td>
    <td><%= link_to "Usuń", admin_user_path(user), :method => :destroy, :confirm => "Na pewno?" %></td>
</tr>
<% end %>
</table>